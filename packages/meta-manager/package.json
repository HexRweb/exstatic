{
  "name": "@exstatic/meta-manager",
  "version": "0.0.1",
  "description": "Manage .exstatic meta (cache and temp) files for Exstatic",
  "author": "HexR <hello@hexr.org>",
  "license": "MIT",
  "repository": "https://github.com/hexrweb/exstatic.git",
  "main": "lib/meta-manager.js",
  "engines": {
    "node": "^10.12.0 || ^12.13.0"
  },
  "files": [
    "lib",
    "yarn.lock"
  ],
  "bugs": "https://github.com/hexrweb/exstatic/issues",
  "homepage": "https://github.com/hexrweb/exstatic",
  "scripts": {
    "lint": "xo",
    "test:unit": "nyc --all --reporter=html --reporter=text mocha --recursive test/unit"
  },
  "dependencies": {
    "@exstatic/utils": "*"
  },
  "devDependencies": {},
  "xo": {
    "rules": {
      "func-names": [
        "error",
        "as-needed"
      ],
      "prefer-object-spread": "off",
      "prefer-spread": "off",
      "no-use-extend-native/no-use-extend-native": "off"
    },
    "overrides": [
      {
        "files": "{packages/**/test,test}/**/*.spec.js",
        "envs": [
          "node",
          "mocha"
        ],
        "rules": {
          "func-names": "off",
          "prefer-arrow-callback": "off",
          "no-unused-expressions": "off",
          "import/no-extraneous-dependencies": "off"
        }
      }
    ]
  }
}
